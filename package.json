{
  "name": "super-firebase-angular",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration=en",
    "start:tr": "ng serve --configuration=tr",
    "build": "ng build",
    "build:i18n": "ng run super-firebase-angular:build:en && ng run super-firebase-angular:build:tr",
    "build:production:i18n": "ng run super-firebase-angular:build:production-en && ng run super-firebase-angular:build:production-tr",
    "server": "ng run super-firebase-angular:server",
    "server:i18n": "ng run super-firebase-angular:server:en && ng run super-firebase-angular:server:tr",
    "server:production:i18n": "ng run super-firebase-angular:server:production:en && ng run super-firebase-angular:server:production:tr",
    "deploy": "firebase deploy --project supermurat-com",
    "test": "ng test",
    "test:cover": "del-cli \"./coverage\" && ng test --watch=false --code-coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "codacy": "cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage -p .",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:functions": "npm --prefix \"./functions\" run lint && npm --prefix \"./functions\" run build",
    "serve": "del-cli \"./functions/dist/*\" && npm run build:functions && npm run build && npm run server && npm run copyToPublic && firebase serve --project supermurat-com",
    "serve:i18n": "del-cli \"./functions/dist/*\" && npm run build:functions && npm run build:i18n && npm run server:i18n && npm run copyToPublic && firebase serve --project supermurat-com",
    "copyToPublic": "del-cli \"./public/*\" && cpy \"./functions/dist/browser/*\" \"./public\" && cpy \"./functions/dist/browser/en/*\" \"./public/en\" && cpy \"./functions/dist/browser/tr/*\" \"./public/tr\" && npm run deleteIndexFromPublic",
    "deleteIndexFromPublic": "del-cli \"./public/index.html\" && del-cli \"./public/en/index.html\" && del-cli \"./public/tr/index.html\"",
    "extract:i18n": "ng xi18n --output-path locale && xliffmerge --profile xliffmerge.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.7",
    "@angular/common": "6.1.7",
    "@angular/compiler": "6.1.7",
    "@angular/core": "6.1.7",
    "@angular/fire": "5.0.2",
    "@angular/forms": "6.1.7",
    "@angular/http": "6.1.7",
    "@angular/platform-browser": "6.1.7",
    "@angular/platform-browser-dynamic": "6.1.7",
    "@angular/platform-server": "6.1.7",
    "@angular/router": "6.1.7",
    "@ng-bootstrap/ng-bootstrap": "3.2.2",
    "bootstrap": "^4.1.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.7",
    "firebase": "^5.5.2",
    "protractor": "~5.4.1",
    "rxjs": "~6.3.2",
    "rxjs-compat": "^6.3.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.1",
    "@angular/cli": "^6.2.1",
    "@angular/compiler-cli": "6.1.7",
    "@angular/language-service": "6.1.7",
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.9.4",
    "angular-tslint-rules": "^1.5.0",
    "codacy-coverage": "^3.1.0",
    "codelyzer": "^4.4.4",
    "coveralls": "^3.0.2",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jquery": "^3.3.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "karma-mocha-reporter": "^2.2.5",
    "ngx-i18nsupport": "^0.17.1",
    "popper.js": "^1.14.4",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "2.9.2"
  }
}
